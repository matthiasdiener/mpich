#! /bin/sh
if [ "$MPIR_HOME" = "" ] ; then 
    MPIR_HOME=#MPIR_HOME#
fi
if [ "$MPIR_HOME" = "#""MPIR_HOME""#" ] ; then
    MPIR_HOME=`pwd`/..
fi
MPIRUN_HOME=$MPIR_HOME/util
if [ "$argsset" = "" ] ; then
   . $MPIRUN_HOME/mpirun.args
   argsset=1
fi
# ch_shmem device
if [ $just_testing != "" ]  then
  if [ "$debugger" != "" ] then
      echo $debugger $progname
  else
      echo $progname $cmdLineArgs -np $np
  fi
else
  if [ "$debugger" != "" ] ; then 
      cat > $PWD_TRIAL/PId$$ <<.
run -np $np $cmdLineArgs
.
      $debugger $progname $commandfile $PWD_TRIAL/PId$$
  else
      $progname $cmdLineArgs -np $np
  fi
fi
