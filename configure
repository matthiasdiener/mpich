            sed -n -e '/^-l/p' -e '/^-L/p' -e '/^P,/p' | \
	    sed -e 's/^P,/-P,/g' | sort | uniq > t1$$f
    else
        cat t1$$f1 | tr ' ' '\012' | \
            sed -n -e '/^-l/p' -e '/^-L/p' -e '/^P,/p' | \
	    sed -e 's/^P,/-P,/g' | sort | uniq > t1$$f
    fi

    # Now, remove common libraries/search paths
