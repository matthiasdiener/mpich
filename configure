\${SEDuA}MPIRUN_DEVICE\${SEDuB}MPIRUN_DEVICE\${SEDuC}$q$DEVICE$q\${SEDuD}
\${SEDeA}MPIRUN_DEVICE\${SEDeB}MPIRUN_DEVICE\${SEDeC}$q$DEVICE$q\${SEDeD}
"
}

