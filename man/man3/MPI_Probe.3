.TH MPI_Probe 3 "9/21/1994" " " "MPI"
.SH NAME
MPI_Probe \- Blocking test for a message

.SH INPUT PARAMETERS
.PD 0
.TP
.B source 
- source rank, or MPI_ANY_SOURCE (integer) 
.PD 1
.PD 0
.TP
.B tag 
- tag value or MPI_ANY_TAG (integer) 
.PD 1
.PD 0
.TP
.B comm 
- communicator (handle) 
.PD 1

.SH OUTPUT PARAMETER
.PD 0
.TP
.B status 
- status object (Status) 
.PD 1
.SH SYNOPSIS
.nf
#include "mpi.h"
int MPI_Probe( source, tag, comm, status )
int         source;
int         tag;
MPI_Comm    comm;
MPI_Status  *status;

.fi

.SH LOCATION
 probe.c
