$(LIB): $(OBJS)
	$(G_RM) -f $(LIB)
	$(G_RM) -f nexus.exp
	/usr/bin/nm -eC $(OBJS) $(PORTS0_OBJS) \
		| /bin/awk -F'|' -f $(SRC_PATH)/ibm_exportlist.awk > nexus.exp
	$(G_CC) -o $(LIBBASE)$(FLAVOR_SUFFIX).$(G_OFILE) \
		$(BASE_OBJS) $(PORTS0_OBJS) \
		-bE:nexus.exp -bM:SRE -T512 -H512 -e _nostart \
		-L/usr/local/qt/lib -lqt -lm \
		-bI:$(SRC_PATH)/ibm_commonsyscalls.exp
	$(G_AR) qv $(LIB) $(STATIC_OBJS) \
		$(LIBBASE)$(FLAVOR_SUFFIX).$(G_OFILE)

