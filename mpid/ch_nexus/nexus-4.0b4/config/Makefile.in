SHELL=/bin/sh
all:
	echo "making config directory"

install:

build_dir_headers:

mostlyclean:
	echo ""

clean:
	echo ""

#
# config.cache is normally in ../, but put it here just to be safe
#
# do we want to put Nexus.defines and Ports0.defines in the dist?
#
